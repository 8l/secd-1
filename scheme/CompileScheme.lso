(LETREC COMPILE
  (COMPILE LAMBDA NIL
    (LEX BUILDLIST))

  (BUILDLIST LAMBDA (X REENTRY)
    (IF (ISNIL X)
        (QUOTE NIL)
        (CONS X (REENTRY BUILDLIST))))


include(LexScheme.lso)
